  <!-- 查询没有成团的列表 -->
  <select id="selectUnFormByPId" parameterType="java.lang.String" resultMap="BaseResultMap">
  	SELECT 
    gp.*
FROM
    group_purcse gp
        LEFT JOIN
    pr_product product ON product.id = gp.PRODUCT_ID
        LEFT JOIN
    group_purcse_member gpm ON gp.id = gpm.GROUP_PURCSE_ID
        LEFT JOIN
    (SELECT 
        (gpm.GROUP_PURCSE_ID) AS GPM_GPID
    FROM
        group_purcse_member AS gpm
    GROUP BY gpm.GROUP_PURCSE_ID
    HAVING COUNT(gpm.GROUP_PURCSE_ID) =1) AS a ON a.GPM_GPID = gpm.GROUP_PURCSE_ID
    where product.id=#{productId,jdbcType=VARCHAR} 
    order by gp.CREATE_DATE asc limit 0,10
  </select>
  
  
  <!-- 查询没有成团的列表 -->
  <select id="selectGroupPurcseByPId" parameterType="map" resultMap="BaseResultMap">
  	SELECT 
    	<include refid="Base_Column_List" />
	FROM
    group_purcse
	WHERE
    	PRODUCT_ID = #{productId,jdbcType=VARCHAR}
        AND DISABLE = #{disable,jdbcType=BIT}
        AND EXPIRED_TIME > (SELECT NOW()) 
