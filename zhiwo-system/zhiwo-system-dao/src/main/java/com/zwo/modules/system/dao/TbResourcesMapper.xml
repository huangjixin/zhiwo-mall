  <select id="selectByUsernameOrMPhone" resultMap="BaseResultMap" parameterType="java.lang.String">
  	SELECT 
    resources.*
FROM
    tb_resources resources
        INNER JOIN
    tb_role_resources rr ON rr.RESOURCES_ID = resources.id
        INNER JOIN
    tb_role role ON rr.ROLE_ID = role.id
        INNER JOIN
    tb_user_group_role ugr ON ugr.ROLE_ID = role.id
        INNER JOIN
    tb_user_group usergroup ON usergroup.id = ugr.USERGROUP_ID
        INNER JOIN
    tb_user user ON user.USERGROUP_ID = usergroup.id
    
    where user.USERNAME=#{usernameOrMPhone} or user.MOBIL_PHONE=#{usernameOrMPhone}
    	   or user.EMAIL=#{usernameOrMPhone}
  </select>
