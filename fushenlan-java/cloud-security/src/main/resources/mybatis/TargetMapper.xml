<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fulan.application.mapper.TargetMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.fulan.api.security.domain.Target">
        <id column="id" property="id" />
        <result column="account_id" property="accountId" />
        <result column="target_year" property="targetYear" />
        <result column="target_time" property="targetTime" />
        <result column="target_value" property="targetValue" />
        <result column="create_time" property="createTime" />
        <result column="create_user" property="createUser" />
        <result column="update_time" property="updateTime" />
        <result column="update_user" property="updateUser" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, account_id AS accountId, target_year AS targetYear, target_time AS targetTime, target_value AS targetValue, create_time AS createTime, create_user AS createUser, update_time AS updateTime, update_user AS updateUser
    </sql>





</mapper>
