  <select id="findRoles" resultType="java.lang.String">
  		SELECT 
		   distinct r.name
		FROM
		    bhm_user u
		        LEFT JOIN
		    bhm_user_role ur ON u.id = ur.USER_ID
		        LEFT JOIN
		    bhm_role r ON r.id = ur.ROLE_ID
		WHERE
		    u.username = #{username}
  </select>
  <select id="findPermissions" resultType="java.lang.String">
	SELECT DISTINCT
	    res.AUTH_NAME
	FROM
		    bhm_user user
		        LEFT JOIN
		    bhm_user_role ur ON user.id = ur.USER_ID
		        LEFT JOIN
		    bhm_role r ON r.id = ur.ROLE_ID
		        LEFT JOIN
		    bhm_role_resource role_res ON role_res.ROLE_ID = r.id
		        LEFT JOIN
		    bhm_resource res ON res.id = role_res.RES_ID
			        
			WHERE
			    user.USERNAME = #{username}	
  </select> 
  <select id="findByUsername" resultMap="UserVoMap">
  		SELECT DISTINCT
		    user.*,r.*,res.*
		FROM
		    bhm_user user
		        LEFT JOIN
		    bhm_user_role ur ON user.id = ur.USER_ID
		        LEFT JOIN
		    bhm_role r ON r.id = ur.ROLE_ID
		        LEFT JOIN
